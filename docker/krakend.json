{
  "version": 3,
  "name": "Krakend-Keycloak Proxy",
  "port": 8000,
  "endpoints": [
    {
      "endpoint": "/auth/token",
      "method": "POST",
      "output_encoding": "json",
      "backend": [
        {
          "url_pattern": "/auth/realms/master/protocol/openid-connect/token",
          "host": ["http://keycloak:8080"],
          "encoding": "form"
        }
      ]
    }
  ]
}
