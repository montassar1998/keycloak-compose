{
  "version": 3,
  "name": "KrakenD-Keycloak Gateway",
  "port": 8000,
  "endpoints": [
    {
      "endpoint": "/auth/{proxy_path+}",
      "output_encoding": "json",
      "backend": [
        {
          "url_pattern": "/{proxy_path+}",
          "host": ["http://keycloak:8080"]
        }
      ]
    }
  ]
}
